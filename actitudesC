    Private Sub ToolStripButton1_Click(sender As Object, e As EventArgs) Handles ToolStripButton1.Click
        NotaComportamiento = Comportamiento / 2.5
        NotaHabilidades = Habilidades / 2.5
        NotaTrabajogrupo = TrabajoGrupo / 2.5
        NotaMotivacion = Motivacion / 2.5

        NotaPerfilActitudinal = (NotaComportamiento + NotaHabilidades + NotaMotivacion + NotaTrabajogrupo) / 4

        PerfilActitudinal.Visible = False
        PerfilActitudinal.Close()
        PerfilActitudinal.Dispose()
        PerfilActitudinal.ShowDialog()

    End Sub

